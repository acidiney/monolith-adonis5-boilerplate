{
  "name": "orion",
  "description": "orion core",
  "author": {
    "email": "acidiney.dias@ideiasdinamicas.com",
    "name": "Acidiney Dias"
  },
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "node ace serve --watch",
    "dev:global": "node ace serve --watch  --encore-args='--host 0.0.0.0'",
    "prebuild": "npx ts-node ./start/prebuild.ts",
    "build": "node ace build --production --ignore-ts-errors",
    "release": "npx standard-version",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts --fix",
    "test": "jest --runInBand --silent --passWithNoTests",
    "test:japa": "node ace test",
    "t:j": "npm run test:japa",
    "test:watch": "npm t -- --watchAll",
    "t:w": "npm run test:watch",
    "test:coverage": "npm t -- --coverage",
    "t:c": "npm run test:coverage",
    "test:verbose": "jest --runInBand --passWithNoTests",
    "t:v": "npm run test:verbose",
    "test:verbose:watch": "npm run t:v -- -- --watchAll",
    "t:v:w": "npm run test:verbose:watch",
    "test:prepare": "NODE_ENV=test node ace migration:fresh && NODE_ENV=test node ace db:sync",
    "cypress:dev:server": "npm NODE_ENV=test node ace serve --encore-args='--host 0.0.0.0'",
    "cypress:open": "concurrently \"npm run cypress:dev:server\" \"cypress open\"",
    "c:o": "npm run cypress:open"
  },
  "devDependencies": {
    "@adonisjs/assembler": "5.9.5",
